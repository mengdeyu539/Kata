#3 Replace With Alphabet Position
def alphabet_position(a):
    b = '0abcdefghijklmnopqrstuvwxyz'
    a=a.replace("'",'').replace('.','').replace(' ','').lower()
    c = []
    for i in a:
        if i in b:
            e = str(b.index(i))
            c.append(e)
    return " ".join(c)
